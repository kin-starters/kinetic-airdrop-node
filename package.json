{
  "name": "nx-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build api --prod",
    "dev": "nx serve api",
    "docker:build": "docker build . -t ghcr.io/kin-starters/kinetic-node-starter",
    "docker:push": "docker push ghcr.io/kin-starters/kinetic-node-starter",
    "docker:run": "docker run -it -p 9876:9876 --env-file ./.env ghcr.io/kin-starters/kinetic-node-starter",
    "generate-keypair": "yarn nx workspace-generator generate-keypair",
    "start": "node dist/api/main.js",
    "test": "nx test"
  },
  "private": true,
  "dependencies": {
    "@kin-kinetic/keypair": "^1.0.0-rc.12",
    "@kin-kinetic/sdk": "^1.0.0-rc.12",
